{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": 0,
    "no-prototype-builtins": 0,
    "semi": "error",
    "import/extensions": 0,
    "no-console": "off"
  }
}
